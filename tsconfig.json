{
  "compilerOptions": {
      "target": "es6",
      "types": ["node"],
      "typeRoots": ["node_modules/@types"],
      "module": "commonjs",
      "outDir": "dist",
      "removeComments": true,
      "strict": true, 
      "skipLibCheck": true,
      "strictNullChecks": true,
      "noImplicitThis": true,
      "inlineSourceMap": true
  
  },
  "include": [
      "src", "src/config/config.ts"
  ],
  "exclude": [
      "node_modules"
  ]
} 